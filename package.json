{
  "name": "gene-neighborhood",
  "version": "1.0.0",
  "description": "Make figures of the gene neighborhood of a MSA.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_PATH=node_modules node_modules/.bin/nyc node_modules/.bin/mocha $(find src -name '*tests.js')",
    "mocha": "NODE_PATH=node_modules node_modules/.bin/mocha $(find src -name '*tests.js')",
    "coverage": "NODE_PATH=node_modules node_modules/.bin/nyc report --reporter=text-lcov | coveralls",
    "build": "mkdir -p public; node_modules/.bin/browserify src/index.js -o public/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theJensenLab/gene-neighborhood.git"
  },
  "keywords": [
    "bioinformatics",
    "genes"
  ],
  "author": "Davi Ortega",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theJensenLab/gene-neighborhood/issues"
  },
  "homepage": "https://github.com/theJensenLab/gene-neighborhood#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "browserify": "^14.5.0",
    "bunyan": "^1.8.12",
    "core-bio": "0.0.7",
    "express": "^4.16.2",
    "multer": "^1.3.0",
    "node-mist3": "^1.3.2",
    "pug": "^2.0.0-rc.4",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1"
  }
}
